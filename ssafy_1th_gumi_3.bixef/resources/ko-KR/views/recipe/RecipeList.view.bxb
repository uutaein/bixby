result-view{
  match: RecipeBasicStructure(result){
    min (Required) max (Many)
  }
  message("최종 결과입니다")
  render{
    list-of(result){
      where-each (item){
        layout-macro (RecipeSummaryLarge){
          param(recipeBasicStructure){
            expression(item)
          }
        }
      }
    }
  }
}