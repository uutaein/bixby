action (GetRecipeByCalories) {
  type(Search)
  description (칼로리를 이용해 레시피를 가져온다)
  collect {
    input (lowerBound) {
      type (LowerBound)
      min (Optional) max (One)
    }
    input (lowerBoundRangeOption) {
      type(LowerBoundRangeOption)
      min(Optional) max (One)
    }
    input(upperBound){
      type(UpperBound)
      min (Optional) max (One)
    }
    input (upperBoundRangeOption) {
      type(UpperBoundRangeOption)
      min(Optional) max (One)
    }
  }
  output (RecipeBasicStructure){
    throws{
      error (InvalidParameterException){
        on-catch{
          halt{
            dialog{
              template (INVALID_PARAMETER_CALORIES)
            }
          }
        }
      }
      unknown-error{
        on-catch{
          halt{
            dialog{
              template (UNKNOWN_ERROR)
            }
          }
        }
      }
    }
  }
}
