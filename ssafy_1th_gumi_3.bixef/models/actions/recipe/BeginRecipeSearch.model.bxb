action (BeginRecipeSearch) {
  description (요리 검색 시작)
  type(BeginTransaction)
  collect {
    input (ingredients) {
      type (Ingredient)
      min (Optional) max (Many)
    }
    
    input(recipeKeyword){
      type(RecipeKeyword)
      min (Optional) max (One)
    }
    
    input(searchKeyword){
      type(SearchKeyword)
      min (Required) max (One)
    }
  }
  output (RecipeCommitState)
}
