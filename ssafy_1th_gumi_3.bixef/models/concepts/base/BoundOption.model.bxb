structure (BoundOption) {
  description (경계값 옵션)
  property (upperBoundFlag){
    type(UpperBoundFlag)
    min (Required) max (One)
  }
  property(upperBound){
    type(UpperBound)
    min (Required) max (One)
  }
  property (lowerBoundFlag){
    type(LowerBoundFlag)
    min (Required) max (One)
  }
  property (lowerBound){
    type(LowerBound)
    min (Required) max (One)
  }
}
